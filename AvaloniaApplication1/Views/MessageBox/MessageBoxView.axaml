<UserControl x:Class="AvaloniaApplication1.Views.MessageBoxView" xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dialogHost="using:DialogHostAvalonia" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:AvaloniaApplication1.ViewModels" d:DesignHeight="450"
             d:DesignWidth="800" x:DataType="vm:MessageBoxViewModel"
             mc:Ignorable="d">
    <Design.DataContext>
        <vm:MessageBoxViewModelFroDesigner />
    </Design.DataContext>
    <StackPanel>
        <TextBox Text="{Binding Message}" />
    </StackPanel>
</UserControl>
